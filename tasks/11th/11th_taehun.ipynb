   "source": [
    "def solution(N):\n",
    "    fib = [0] * (N+1)\n",
    "    fib[0] = 4\n",
    "    fib[1] = 6\n",
    "    for i in range(2, N):\n",
    "        fib[i] = fib[i-1] + fib[i-2]\n",
    "    \n",
    "    return fib[N-1]\n",
    "\n",
    "print(solution(5))\n",
    "print(solution(6))\n",
    "print(solution(1))\n",
    "print(solution(80))"
   ]
