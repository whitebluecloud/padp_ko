    "def series_sum(n):\n",
    "    # Happy Coding ^_^\n",
    "    a = 1.00\n",
    "    b = 0.00\n",
    "    for i in range(0, n-1):\n",
    "        a = a + 3.00\n",
    "        b = b + (1.00/a)\n",
    "    if n == 0:\n",
    "        return \"%.2f\"%b\n",
    "    else :\n",
    "        c = \"%.2f\"%(1+b)\n",
    "        return c\n",
    "    "
